    {
      "local": "secondary-droplet/bin/bwb_status_monitor.py",
      "remote": "/usr/local/bin/bwb_status_monitor.py",
      "mode": "755"
    },
    {
      "local": "secondary-droplet/systemd/yt-restapi.service",
      "remote": "/etc/systemd/system/yt-restapi.service",
      "mode": "644"
    },
    {
      "local": "secondary-droplet/config/logrotate/bwb-services",
      "remote": "/etc/logrotate.d/bwb-services",
      "mode": "644"
    }
  ]
