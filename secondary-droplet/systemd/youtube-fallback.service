[Unit]
Description=YouTube Secondary (Backup) - Slate 720p com imagem e textos
Wants=network-online.target
After=network-online.target
StartLimitIntervalSec=30
StartLimitBurst=10

[Service]
Type=simple
EnvironmentFile=-/etc/youtube-fallback.env
ExecStart=/usr/local/bin/youtube_fallback.sh
Restart=always
RestartSec=3
Nice=5
LimitNOFILE=1048576
KillMode=control-group
KillSignal=SIGTERM
TimeoutStopSec=5

[Install]
WantedBy=multi-user.target
